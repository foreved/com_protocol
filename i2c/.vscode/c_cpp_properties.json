{
    "configurations": [
        {
            "name": "Linux",
            "includePath": [
                "${workspaceFolder}/**",
                "./Core/Inc",
                "./Drivers/STM32F1xx_HAL_Driver/Inc",
                "./Drivers/CMSIS/Device/ST/STM32F1xx/Include",
                "./Drivers/CMSIS/Include",
                "../libs/include"
            ],
            "defines": [
                "USE_FULL_LL_DRIVER",
                "HSE_VALUE=8000000",
                "HSE_STARTUP_TIMEOUT=100",
                "LSE_STARTUP_TIMEOUT=5000",
                "LSE_VALUE=32768",
                "HSI_VALUE=8000000",
                "LSI_VALUE=40000",
                "VDD_VALUE=3300",
                "PREFETCH_ENABLE=1",
                "STM32F103xB"
            ],
            "compilerPath": "/opt/arm-none-eabi/bin/arm-none-eabi-gcc",
            "cStandard": "c17",
            "cppStandard": "gnu++17",
            "intelliSenseMode": "linux-gcc-x64"
        }
    ],
    "version": 4
}